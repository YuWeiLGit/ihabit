<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/Theme_Twine">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="39dp"
        app:layout_constraintGuide_percent="0.2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="170dp"
        app:layout_constraintGuide_percent="0.37" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="268dp"
        app:layout_constraintGuide_percent="0.54" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="314dp"
        app:layout_constraintGuide_percent="0.8" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/storeframe1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView8u"
        android:layout_width="70dp"
        android:layout_height="64dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15u"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline35"
        app:layout_constraintTop_toTopOf="@+id/imageView15u"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/btn_start_n" />

    <ImageView
        android:id="@+id/imageView12u"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_margin="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline32"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/frame" />

    <ImageView
        android:id="@+id/itemPicUpgrade"
        android:layout_width="36dp"
        android:layout_height="46dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView12u"
        app:layout_constraintEnd_toEndOf="@+id/imageView12u"
        app:layout_constraintStart_toStartOf="@+id/imageView12u"
        app:layout_constraintTop_toTopOf="@+id/imageView12u"
        app:layout_constraintVertical_bias="0.416"
        app:srcCompat="@drawable/armor_5" />

    <ImageView
        android:id="@+id/imageView15u"
        android:layout_width="70dp"
        android:layout_height="64dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16u"
        app:layout_constraintEnd_toStartOf="@+id/guideline34"
        app:layout_constraintStart_toStartOf="@+id/guideline33"
        app:layout_constraintTop_toTopOf="@+id/imageView16u"
        app:srcCompat="@drawable/btn_start_n" />

    <ImageView
        android:id="@+id/imageView16u"
        android:layout_width="72dp"
        android:layout_height="39dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView12u"
        app:layout_constraintEnd_toStartOf="@+id/guideline33"
        app:layout_constraintStart_toStartOf="@+id/guideline32"
        app:layout_constraintTop_toTopOf="@+id/imageView12u"
        app:srcCompat="@drawable/loading_bg" />

    <ImageView
        android:id="@+id/upgradeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16u"
        app:layout_constraintEnd_toEndOf="@+id/imageView16u"
        app:layout_constraintStart_toStartOf="@+id/imageView16u"
        app:layout_constraintTop_toTopOf="@+id/imageView16u"
        app:srcCompat="@drawable/levelup" />

    <TextView
        android:id="@+id/oldGradeNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/_100"
        android:textColor="@color/white"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15u"
        app:layout_constraintEnd_toEndOf="@+id/imageView15u"
        app:layout_constraintStart_toStartOf="@+id/imageView15u"
        app:layout_constraintTop_toTopOf="@+id/imageView15u" />

    <TextView
        android:id="@+id/newGradeNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/unknmw"
        android:textColor="@color/white"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8u"
        app:layout_constraintEnd_toEndOf="@+id/imageView8u"
        app:layout_constraintStart_toStartOf="@+id/imageView8u"
        app:layout_constraintTop_toTopOf="@+id/imageView8u" />

    <ProgressBar
        android:id="@+id/progessUp"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="87dp"
        android:layout_height="22dp"
        android:layout_marginStart="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15u"
        app:layout_constraintStart_toStartOf="@+id/guideline34"
        app:layout_constraintTop_toTopOf="@+id/imageView15u"
        app:layout_constraintVertical_bias="0.476" />

    <ImageView
        android:id="@+id/resultUp"
        android:layout_width="45dp"
        android:layout_height="44dp"
        android:layout_marginStart="64dp"
        app:layout_constraintBottom_toBottomOf="@+id/progessUp"
        app:layout_constraintEnd_toStartOf="@+id/guideline35"
        app:layout_constraintStart_toEndOf="@+id/imageView15u"
        app:layout_constraintTop_toTopOf="@+id/progessUp"
        app:srcCompat="@drawable/ic_baseline_arrow_right_24" />

    <ImageView
        android:id="@+id/imageView60u"
        android:layout_width="27dp"
        android:layout_height="21dp"
        app:layout_constraintBottom_toTopOf="@+id/progessUp"
        app:layout_constraintEnd_toEndOf="@+id/progessUp"
        app:layout_constraintHorizontal_bias="0.363"
        app:layout_constraintStart_toStartOf="@+id/progessUp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/actor_coin" />

    <TextView
        android:id="@+id/priceToUpgrade"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/_20"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageView60u"
        app:layout_constraintStart_toEndOf="@+id/imageView60u"
        app:layout_constraintTop_toTopOf="@+id/imageView60u"
        app:layout_constraintVertical_bias="0.538" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/upgrade"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/guideline33"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="@+id/guideline32"
        app:layout_constraintTop_toBottomOf="@+id/imageView16u" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/uplv"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/progessUp"
        app:layout_constraintStart_toStartOf="@+id/progessUp"
        app:layout_constraintTop_toBottomOf="@+id/progessUp" />

</androidx.constraintlayout.widget.ConstraintLayout>